<% // Render your search results here %>
<% // This is how to do comments in EJS, by the way... %>

<h2>Results for <%= input %>:</h2>
<% search.forEach(result => { %>
  <div>
    <a class="title-link" href="/movies/<%= result.imdbID %>">
      <h3>
        <%= result.Title %>
      </h3>
      <img class="tmb" src="<%= result.Poster %>" alt="<%= result.Title %> poster" />
    </a>
  </div>
<% }) %>